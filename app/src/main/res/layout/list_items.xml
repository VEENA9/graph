<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp"
    android:background="@drawable/temp">

    <TextView
        android:id="@+id/textViewName"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Large"
        android:textColor="#000000" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="89dp"
        android:layout_height="87dp"
        android:layout_alignEnd="@+id/btnObjRequest"
        android:layout_alignTop="@+id/textViewName"
        android:background="@drawable/my1"
        android:layout_alignRight="@+id/btnObjRequest" />
    <!--app:srcCompat="@drawable/user"-->

    <TextView
        android:id="@+id/textViewImageUrl"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignStart="@+id/textViewName"
        android:autoLink="web"
        android:textColor="#08308e"
        android:layout_alignLeft="@+id/textViewName" />


    <Button
        android:id="@+id/btnObjRequest"
        android:layout_width="363dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:background="#92c6f7"
        android:text="More" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="197dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textViewName"
        android:layout_marginStart="0dp"
        android:text="0018"
        android:textColor="#141174"
        android:background="#eef1f4"
        android:textSize="20dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="0dp" />

</RelativeLayout>
